{
	"trailingComma": "es5",
	"tabWidth": 2,
	"semi": false,
	"singleQuote": true,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"arrowParens": "always",
	"useTabs": true,
	"quoteProps": "consistent",
	"singleAttributePerLine": true,
	"printWidth": 150,
	"plugins": ["prettier-plugin-tailwindcss"]
}
